.box{
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #F3F2F7;
}
.box_type{
  background-color: #ffffff;
  padding: 20rpx 0 0 40rpx;
  color: #282C3F;
}
.flex{
  margin-top: 20rpx;
  border-bottom: 1rpx solid #F3F2F7;
}
.item{  
  display: inline-block; 
  padding: 10rpx 20rpx;
  background-color: #F7F7FA;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  color: #9FA3BB;
  border-radius: 4rpx;
}   
.service_selection .is_checked {  
  color: #ffffff;  
  background: #FE8166;  
}  
.box_type_limit{
  color: #282C3F;
  font-size: 28rpx;
  line-height: 88rpx;
}
.box_type_limit_num{
  float: right;
  margin-right: 60rpx;
}
.box_title{
  margin-top: 20rpx;
  background-color: #ffffff;
  padding: 20rpx 40rpx;
}
.box_information{
  margin-top: 20rpx;
  background-color: #ffffff;
  padding: 20rpx 0 0 40rpx;
}
.box_information_people{
  line-height: 86rpx;
  border-bottom: 1rpx solid #F3F2F7;
  position: relative;
}
.box_information_people_text{
  float: right;
  margin-right: 60rpx;
  color:#616686;
}
.displayAll{
  margin-left: 40rpx;
  margin-top: 20rpx;
  color: #3296FA;
}
.submit{
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: #ffffff;
  line-height: 100rpx;
  color: #3296FA;
}
.box_show{
  background-color: #ffffff;
  padding-left: 40rpx;
  padding-bottom: 100rpx;
}
.box_information_people_kuai{
  background-color: #F7F7FA;
  display: inline-block;
  padding: 10rpx 20rpx;
  line-height: 30rpx;
  color: #9FA3BB;
  font-size: 20rpx;
  border-radius: 6rpx;
  margin-right: 20rpx;
}
input{
  display: inline-block;
  margin-top: 20rpx;
  margin-right: 41rpx;
  float: right;
  text-align: right; 
}
picker{
  display: inline-block;
  margin-right: 41rpx;
  float: right;
  text-align: right; 
}
.enterNextImg {
  width:20rpx;
  height:30rpx;
  position:absolute;
  right:15rpx;
  top:30rpx;

}
.picker{
  color:#616686;
}