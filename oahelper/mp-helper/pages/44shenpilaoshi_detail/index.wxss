/* pages/22xinxishenpi/index.wxss */
.top{ line-height: 86rpx; padding: 0 10rpx; color: #FB7F03; font-size: 28rpx; border-bottom: #E5E5E5 solid 2rpx; background-color: #fff;}
.top::before{ content: ""; display: inline-block; vertical-align: middle; position: relative; top: -2rpx; width: 8rpx; height: 28rpx; margin-right: 18rpx; background-color: #FF9900;}

.center{ padding: 0 30rpx 20rpx 30rpx; background-color: #fff;}

.bottom{ margin-top: 0rpx;}

.center_choose{ height: 90rpx; line-height: 88rpx; color: #000; border-bottom: #E5E5E5 solid 2rpx; position: relative;}
.center_choose.no_line{ border-bottom: transparent solid 2rpx;}
.center_choose_ico{ display: inline-block; vertical-align: middle; width: 44rpx; height: 44rpx; line-height: 44rpx; margin-right: 10rpx; text-align: center; color: #fff; border-radius: 50%; border: #C9C9C9 solid 2rpx; position: relative; top: -2rpx;}
.center_choose_ico .iconfont{ display: none;}
.center_choose_ico_in{ border: #FF9900 solid 2rpx; background-color: #FF9900;}
.center_choose_ico_in .iconfont{ display: inline-block;}

.back_text{ display: block; width: 100%; height: 170rpx; padding-left: 68rpx; padding-top: 20rpx; box-sizing: border-box;}

.send_btn{ margin-bottom: 60rpx;}

.center .choose_img{ display: inline-block; margin-right: 45rpx; margin-bottom: 60rpx;}

.tuihui_detail{ margin-bottom: 20rpx; padding: 10rpx 30rpx 30rpx 30rpx; line-height: 48rpx; font-size: 30rpx; background-color: #fff;}
.tuihui_detail text{ color: #888;}

.help {
  background-color: #fff;
  margin: 0 auto;
}
.help .help_item {
  margin: 10rpx auto;
}
.help .help_item .title {
  font-size: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  display: flex;
  color: #FB7F03;
}
.help .help_item .title .title_1 {
  width: 630rpx;
  height: 60rpx;
}
.help .help_item .title .title_2 {
  width: 50rpx;
  height: 60rpx;
  text-align: center;
}
.help .help_item .title .title_2 image {
  width: 35rpx;
  height: 35rpx;
}
.help .help_item .detail {
  margin: 10rpx auto;
  font-size: 25rpx;
  line-height: 40rpx;
  text-indent: 2em;
}

.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  align-items: center;
}
.td {
    justify-content: center;
    word-wrap: break-word;
    word-break: break-all;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}