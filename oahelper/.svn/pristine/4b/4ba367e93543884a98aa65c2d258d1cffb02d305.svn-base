.logo {
  text-align: center;
}

.logo image {
  display: block;
  margin: 100rpx auto;
  width: 260rpx;
  height: 260rpx;
}

.form{
  width: 90%;
}

.form_label {
  height: 28rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(169, 169, 186, 1);
  line-height: 28rpx;
}

.form_item {
  height: 80rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 12rpx;
  opacity: 0.8;
  border: 1px solid rgba(136, 136, 136, 1);
}


.commit_button {
  height: 100rpx;
  background: rgba(251, 167, 3, 1);
  border-radius: 10rpx;
  border: 1px solid;
  width: 80%;
  font-size: 40rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 85rpx;
  margin-top: 30rpx;
}
