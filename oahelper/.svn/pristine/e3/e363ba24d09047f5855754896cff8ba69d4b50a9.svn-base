.news-item-text{
    word-break:keep-all;
    white-space:nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    font-weight: 500;
    cursor: pointer;
    height: 30px;

    .height-100{
        height: 100%;
    }
    .infor-icon-row{
        color: #c8c8c8;
    }

    .groupLabel{
        font-weight: bold;
    }
    .timeLabel{
        text-align: right;
    }
    /**1 中
        16 敏
        45 正
        46 负
        **/
    .cat1{
        color:#2b85e4;
    }    .cat16{
        color:#c09853;
    }    .cat45{
        color:#183691;
    }    .cat46{
