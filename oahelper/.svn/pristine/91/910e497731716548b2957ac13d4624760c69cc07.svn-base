/* pages/22xinxishenpi/index.wxss */
.top{ line-height: 86rpx; padding: 0 30rpx; color: #FB7F03; font-size: 28rpx; border-bottom: #E5E5E5 solid 2rpx; background-color: #fff;}
.top::before{ content: ""; display: inline-block; vertical-align: middle; position: relative; top: -2rpx; width: 8rpx; height: 28rpx; margin-right: 18rpx; background-color: #FF9900;}

.center{ padding: 0 30rpx 40rpx 30rpx; background-color: #fff;}

.bottom{ margin-top: 60rpx;}

.center_choose{ height: 90rpx; line-height: 88rpx; color: #000; border-bottom: #E5E5E5 solid 2rpx; position: relative;}
.center_choose.no_line{ border-bottom: transparent solid 2rpx;}
.center_choose_ico{ display: inline-block; vertical-align: middle; width: 44rpx; height: 44rpx; line-height: 44rpx; margin-right: 10rpx; text-align: center; color: #fff; border-radius: 50%; border: #C9C9C9 solid 2rpx; position: relative; top: -2rpx;}
.center_choose_ico .iconfont{ display: none;}
.center_choose_ico_in{ border: #FF9900 solid 2rpx; background-color: #FF9900;}
.center_choose_ico_in .iconfont{ display: inline-block;}

.back_text{ display: block; width: 100%; height: 170rpx; padding-left: 68rpx; padding-top: 20rpx; box-sizing: border-box;}

.send_btn{ margin-bottom: 60rpx;}

