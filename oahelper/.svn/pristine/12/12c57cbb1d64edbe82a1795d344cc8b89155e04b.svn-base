/* pages/17wenjian_list/index.wxss */
page{ background-color: #fff;
font-family: "PingFangSC-Medium", "PingFangSC-Regular", "sans-serif", "STHeitiSC-Light", "微软雅黑", "Microsoft YaHei";
}
.center{ padding: 20rpx 30rpx 0 30rpx;}

.msg{ padding: 0 30rpx; height: 80rpx; line-height: 80rpx; color: #FB7F03; font-size: 30rpx; background-color: #FFEBCD;}

.list_box{ padding: 20rpx 30rpx;}
.list{ padding: 18rpx 0; border-bottom: #E5E5E5 solid 2rpx;}
.list > view:first-child{ height: 62rpx; line-height: 62rpx; padding-right: 70rpx; position: relative;}
.list > view:first-child view{ white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
.list > view:first-child .iconfont{ width: 62rpx; height: 62rpx; text-align: center; position: absolute; right: 0; top: 0;}
.list > view:first-child .iconfont.icon-tubiao_wuxing-1{ color:#D1D1D1;}
.list > view:first-child .iconfont.icon-tubiao_wuxing-{ color:#FF9900;}

.list > view:last-child{ height: 46rpx; line-height: 46rpx; color: #3271FF; font-size: 28rpx; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
.list > view:last-child.list_hover{ color: #FBA703;}

.no_list{ padding-top: 100rpx; text-align: center; color: #999; font-size: 32rpx;}
.no_list image{ width: 320rpx;}



