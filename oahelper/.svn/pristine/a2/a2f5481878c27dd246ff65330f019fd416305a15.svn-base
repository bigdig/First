package com.tfzq.util.excel;

public class CellData {
	private int row;
	private int column;
	private String text;

	public CellData(int row, int column, String text) {
		super();
		this.row = row;
		this.column = column;
		this.text = text;
	}

	public int getRow() {
		return row;
	}

	public void setRow(int row) {
		this.row = row;
	}

	public int getColumn() {
		return column;
	}

	public void setColumn(int column) {
		this.column = column;
	}

	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}

}
