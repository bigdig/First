/* pages/10tongzhi_list/index.wxss */
page{ background-color: #fff;
font-family: "PingFangSC-Medium", "PingFangSC-Regular", "sans-serif", "STHeitiSC-Light", "微软雅黑", "Microsoft YaHei";
}
.center{ padding: 20rpx 30rpx;}
.list_box{ padding-top: 10rpx;}
.list{ padding: 18rpx 0; border-bottom: #E5E5E5 solid 2rpx;font-size: 32rpx;}
.list > view:first-child{ height: 62rpx; line-height: 62rpx; padding-right: 70rpx; position: relative;}
.list > view:first-child navigator{ white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
.list > view:first-child .iconfont{ width: 62rpx; height: 62rpx; text-align: center; position: absolute; right: 0; top: 0;}
.list > view:first-child .iconfont.icon-tubiao_wuxing-1{ color:#D1D1D1;}
.list > view:first-child .iconfont.icon-tubiao_wuxing-{ color:#FF9900;}

.list > view:last-child{ height: 46rpx; line-height: 46rpx; color: #999; font-size: 24rpx;}
.list_hover{ color: #FBA703;}

.no_list{ padding-top: 100rpx; text-align: center; color: #999; font-size: 28rpx;}
.no_list image{ width: 320rpx;}
.get_more{font-size: 28rpx;}


