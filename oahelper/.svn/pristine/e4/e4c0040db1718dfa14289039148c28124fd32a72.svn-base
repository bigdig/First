/* pages/meeting-record/index.wxss */

.record {
  width: 100%;
  height: 40rpx;
  background: rgba(255, 255, 255, 1);
  padding-left: 40rpx;
  padding-top: 20rpx;
}

.item {
  height: 30rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 30rpx;
}

.date {
  margin-right: 20rpx;
}

.remark {
  height: 28rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  line-height: 28rpx;
}
