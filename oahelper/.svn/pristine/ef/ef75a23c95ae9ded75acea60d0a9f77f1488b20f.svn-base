.title {
  width: 100%;
  height: 100rpx;
  background: rgba(255, 255, 255, 1);
  padding-left: 40rpx;
  padding-top: 40rpx;
  padding-bottom: 40rpx;
}

.title_text {
  height: 60rpx;
  font-size: 48rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 60rpx;
}

.title_end_time {
  height: 60rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  line-height: 60rpx;
  margin-top: 30rpx;
}

.vote {
  width: 100%;
  margin-top: 10rpx;
  padding-left: 40rpx;
  padding-bottom: 30rpx;
  background: rgba(255, 255, 255, 1);
}

.vote_title {
  height: 48rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
  margin-top: 30rpx;
}

.vote_item {
  height: 44rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  line-height: 34rpx;
  margin-top: 40rpx;
}

.vote_item :hover {
  background-color: #fba703;
}

.commit_button {
  height: 100rpx;
  background: rgba(251, 167, 3, 1);
  border-radius: 10rpx;
  border: 1px solid;
  width: 80%;
  font-size: 40rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 85rpx;
  margin-top: 80rpx;
  margin-bottom: 80rpx;
}
