@SuppressWarnings("serial")
public class GoInformUserBean extends GoInformUser{
    private String isReadText;
    private String isRepliedText;
    private GoInform inform;
    
    public GoInform getInform() {
		return inform;
	}
	public void setInform(GoInform inform) {
		this.inform = inform;
	}
	public void setIsReadText(String isReadText){
        this.isReadText = isReadText;
